server {
    listen 80;
    listen [::]:80;
    listen 443 ssl;
    listen [::]:443 ssl;
    http2  on;
    server_name metaserv.huycao.online;
    ssl_certificate /etc/nginx/ssl/live/metaserv.huycao.online/fullchain.pem;
    ssl_certificate_key /etc/nginx/ssl/live/metaserv.huycao.online/privkey.pem;

    location / {
        proxy_pass http://103.20.144.134:3000;
    }
}

server {
    listen 80;
    listen [::]:80;
    listen 443;
    listen [::]:443;
    server_name cms.huycao.online;

    location / {
        proxy_pass http://103.20.144.134:3001;
    }
}